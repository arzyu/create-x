{
  "name": "create-x",
  "version": "0.1.0",
  "description": "Create project from template.",
  "bin": {
    "create-x": "dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "create-x": "ts-node src/index.ts",
    "create-x:debug": "node --inspect -r ts-node/register src/index.ts",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "postbuild": "chmod +x dist/index.js",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@types/empty-dir": "^1.0.0",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^7.1.1",
    "@types/inquirer": "0.0.43",
    "@types/uuid": "^3.4.4",
    "chalk": "^2.4.1",
    "commander": "^2.19.0",
    "empty-dir": "^1.0.0",
    "fs-extra": "^7.0.1",
    "get-package-info": "github:arzyu/get-package-info#v0.1.0",
    "glob": "^7.1.3",
    "inquirer": "^6.2.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arzyu/create-x"
