    "@types/uuid": "^3.4.5",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "empty-dir": "^2.0.0",
    "fs-extra": "^8.1.0",
    "get-package-info": "github:arzyu/get-package-info#v0.1.0",
    "glob": "^7.1.4",
    "inquirer": "^6.5.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "repository": {
    "type": "git",
